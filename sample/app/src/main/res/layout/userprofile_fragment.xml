<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    android:id="@+id/userProfile_constraintLayout_container"
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:paddingTop="16dp"
    android:paddingLeft="16dp"
    android:paddingRight="16dp">
    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintVertical_bias="0"
        android:fillViewport="true">
        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <TextView
                android:id="@+id/userProfile_textView_identity"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintHorizontal_bias="0"
                app:layout_constraintVertical_bias="0"
                style="@style/TextAppearance.AppCompat.Large"
                android:text="Identity"
                />
            <com.google.android.material.textfield.TextInputLayout
                android:id="@+id/userProfile_textInputLayout_identityKey"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox"
                app:layout_constraintTop_toBottomOf="@id/userProfile_textView_identity"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintVertical_bias="0">
                <EditText
                    android:id="@+id/userProfile_editText_identityKey"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:hint="Key"
                    android:imeOptions="actionDone"
                    android:lines="1"
                    android:scrollbars="horizontal"
                    android:scrollHorizontally="true"
                    android:inputType="none"
                    android:editable="false"
                    android:singleLine="true"/>
            </com.google.android.material.textfield.TextInputLayout>
            <com.google.android.material.textfield.TextInputLayout
                android:id="@+id/userProfile_textInputLayout_identityValue"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox"
                app:layout_constraintTop_toBottomOf="@id/userProfile_textInputLayout_identityKey"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintVertical_bias="0"
                >
                <EditText
                    android:id="@+id/userProfile_editText_identityValue"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:hint="Value"
                    android:imeOptions="actionDone"
                    android:lines="1"
                    android:scrollbars="horizontal"
                    android:scrollHorizontally="true"
                    android:inputType="text"
                    android:singleLine="true"/>
            </com.google.android.material.textfield.TextInputLayout>
            <Button
                android:id="@+id/userProfile_button_selectPredefinedIdentityKey"
                style="@style/Widget.AppCompat.Button.Colored"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                app:layout_constraintTop_toBottomOf="@id/userProfile_textInputLayout_identityValue"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toLeftOf="@id/userProfile_button_identitySet"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintHorizontal_bias="0"
                app:layout_constraintHorizontal_weight="1"
                app:layout_constraintVertical_bias="0"
                android:text="Select Predefined Key"
                />
            <Button
                android:id="@+id/userProfile_button_identitySet"
                style="@style/Widget.AppCompat.Button.Colored"
                android:layout_width="140dp"
                android:layout_height="wrap_content"
                app:layout_constraintTop_toBottomOf="@id/userProfile_textInputLayout_identityValue"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintHorizontal_bias="1"
                app:layout_constraintVertical_bias="0"
                android:text="Set"
                />
            <TextView
                android:id="@+id/userProfile_textView_userProfile"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintTop_toBottomOf="@id/userProfile_button_identitySet"
                app:layout_constraintHorizontal_bias="0"
                app:layout_constraintVertical_bias="0"
                style="@style/TextAppearance.AppCompat.Large"
                android:text="User Profile"
                />
            <com.google.android.material.textfield.TextInputLayout
                android:id="@+id/userProfile_textInputLayout_userProfileKey"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox"
                app:layout_constraintTop_toBottomOf="@id/userProfile_textView_userProfile"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintVertical_bias="0">
                <EditText
                    android:id="@+id/userProfile_editText_userProfileKey"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:hint="Key"
                    android:imeOptions="actionDone"
                    android:lines="1"
                    android:scrollbars="horizontal"
                    android:scrollHorizontally="true"
                    android:inputType="text"
                    android:singleLine="true"/>
            </com.google.android.material.textfield.TextInputLayout>
            <com.google.android.material.textfield.TextInputLayout
                android:id="@+id/userProfile_textInputLayout_userProfileValue"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox"
                app:layout_constraintTop_toBottomOf="@id/userProfile_textInputLayout_userProfileKey"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintVertical_bias="0"
                >
                <EditText
                    android:id="@+id/userProfile_editText_userProfileValue"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:hint="Value"
                    android:imeOptions="actionDone"
                    android:lines="1"
                    android:scrollbars="horizontal"
                    android:scrollHorizontally="true"
                    android:inputType="text"
                    android:singleLine="true"/>
            </com.google.android.material.textfield.TextInputLayout>
            <Button
                android:id="@+id/userProfile_button_selectPredefinedUserProfileKey"
                style="@style/Widget.AppCompat.Button.Colored"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                app:layout_constraintTop_toBottomOf="@id/userProfile_textInputLayout_userProfileValue"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toLeftOf="@id/userProfile_button_userProfileSet"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintHorizontal_bias="0"
                app:layout_constraintHorizontal_weight="1"
                app:layout_constraintVertical_bias="0"
                android:text="Select Predefined Key"
                />
            <Button
                android:id="@+id/userProfile_button_userProfileSet"
                style="@style/Widget.AppCompat.Button.Colored"
                android:layout_width="140dp"
                android:layout_height="wrap_content"
                app:layout_constraintTop_toBottomOf="@id/userProfile_textInputLayout_userProfileValue"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintHorizontal_weight="1"
                app:layout_constraintHorizontal_bias="1"
                app:layout_constraintVertical_bias="0"
                android:text="Set"
                />
            <TextView
                android:id="@+id/userProfile_textView_notificationFlag"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintTop_toBottomOf="@id/userProfile_button_userProfileSet"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintHorizontal_bias="0"
                app:layout_constraintVertical_bias="0"
                style="@style/TextAppearance.AppCompat.Large"
                android:text="User Profile - Notification"
                />
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintTop_toTopOf="@id/userProfile_spinner_notificationType"
                app:layout_constraintBottom_toBottomOf="@id/userProfile_spinner_notificationType"
                app:layout_constraintHorizontal_bias="0"
                style="@style/TextAppearance.AppCompat.Medium"
                android:text="Type"
                />
            <Spinner
                android:id="@+id/userProfile_spinner_notificationType"
                android:layout_width="wrap_content"
                android:layout_height="30dp"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintTop_toBottomOf="@id/userProfile_textView_notificationFlag"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintHorizontal_bias="1"
                app:layout_constraintVertical_bias="0"
                />
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintTop_toTopOf="@id/userProfile_spinner_notificationChannel"
                app:layout_constraintBottom_toBottomOf="@id/userProfile_spinner_notificationChannel"
                app:layout_constraintHorizontal_bias="0"
                style="@style/TextAppearance.AppCompat.Medium"
                android:text="Channel"
                />
            <Spinner
                android:id="@+id/userProfile_spinner_notificationChannel"
                android:layout_width="wrap_content"
                android:layout_height="30dp"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintTop_toBottomOf="@id/userProfile_spinner_notificationType"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintHorizontal_bias="1"
                app:layout_constraintVertical_bias="0"
                />
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintTop_toTopOf="@id/userProfile_checkBox_subscriptionStatus"
                app:layout_constraintBottom_toBottomOf="@id/userProfile_checkBox_subscriptionStatus"
                app:layout_constraintHorizontal_bias="0"
                style="@style/TextAppearance.AppCompat.Medium"
                android:text="Subscription Status"
                />
            <CheckBox
                android:id="@+id/userProfile_checkBox_subscriptionStatus"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintTop_toBottomOf="@id/userProfile_spinner_notificationChannel"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintHorizontal_bias="1"
                app:layout_constraintVertical_bias="0"
                />
            <Button
                android:id="@+id/userProfile_button_notificationSet"
                style="@style/Widget.AppCompat.Button.Colored"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:layout_constraintTop_toBottomOf="@id/userProfile_checkBox_subscriptionStatus"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintVertical_bias="0"
                android:text="Set"
                />
        </androidx.constraintlayout.widget.ConstraintLayout>
    </ScrollView>

</androidx.constraintlayout.widget.ConstraintLayout>